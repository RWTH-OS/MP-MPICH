        { echo "$as_me:$LINENO: WARNING: Error in creating test program for ranlib test!" >&5
echo "$as_me: WARNING: Error in creating test program for ranlib test!" >&2;}
    else
	# Check that we can link the program
	echo $CLINKER $CFLAGS $LDFLAGS conftest.o -o conftest foo.a $LIBS >> \
		config.log
        if eval $CLINKER $CFLAGS $LDFLAGS conftest.o -o conftest foo.a $LIBS \
		>>config.log 2>&1 ; then
	    { echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }
	else
	    { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
	    { echo "$as_me:$LINENO: WARNING: Error linking with ranlibed library." >&5
echo "$as_me: WARNING: Error linking with ranlibed library." >&2;}
	    broken=1
        fi
    fi
    /bin/rm -f foo.a
    if test $broken = 1 ; then
	{ echo "$as_me:$LINENO: WARNING: RANLIB ($RANLIB) failed! Assuming that ranlib is a stub
		returning non-zero condition code." >&5
echo "$as_me: WARNING: RANLIB ($RANLIB) failed! Assuming that ranlib is a stub
		returning non-zero condition code." >&2;}
        RANLIB=':'
    fi
fi

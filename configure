    "bin/mpif77":F) chmod +x "$ac_file" ;;
    "bin/mpif90":F) chmod +x "$ac_file" ;;
    "$mpiruntarget":F) chmod +x $mpiruntarget ;;
    "doc/port":F) chmod +x "$ac_file" ;;
    "doc/portbuild":F) chmod +x "$ac_file" ;;
    "doc/portperf":F) chmod +x "$ac_file" ;;

# Test again in case we failed to build a simple program
if test "x$F77" != "x" ; then
    #
    # Grumph.  Here are a punch of different approaches
    # We have several axes the check:
    # Name of routine
    # Library to link with (none, -lU77 (HPUX), -lg2c (LINUX f77))
    # -lPEPCF90 (Intel ifc)
    trials="0 1 2 3 4 5 6"
    # "0" means (none)
    triallibs="0 -lU77 -lg2c -lPEPCF90"
    # Trialopts is for systems where you
    # need to add *compile-time* options to get command line arguments to
    # work.
    # +U77 is needed by HP Fortran to access getarg etc.
    # The -N109 was used for getarg before we realized that GETARG
    # was necessary with the (non standard conforming) Absoft compiler
    # (Fortran is monocase; Absoft uses mixedcase by default)
    trialopts="0 +U77 -N109"
    #
    echo "$as_me:$LINENO: checking how to get command-line arguments in Fortran" >&5
echo $ECHO_N "checking how to get command-line arguments in Fortran... $ECHO_C" >&6
    msg_yes="no"
    for trial in $trials ; do
	if test "$msg_yes" = "yes" ; then break ; fi
        case $trial in
	0) # User-specified values, if any
	   if test -z "$F77GETARG" -o -z "$F77IARGC" ; then continue ; fi
	   { echo "$as_me:$LINENO: Trying environment variables F77GETARG etc." >&5
echo "$as_me: Trying environment variables F77GETARG etc." >&6;}
	   ;;
	1) # Standard practice, uppercase (some compilers are case-sensitive)
	   F90UNIX=""
	   F77GETARGDECL="external GETARG"
	   F77GETARG="call GETARG(i,s)"
	   F77IARGC="IARGC()"
	   { echo "$as_me:$LINENO: Trying GETARG and IARGC" >&5
echo "$as_me: Trying GETARG and IARGC" >&6;}
	   ;;
	2) # Standard practice, lowercase
	   F90UNIX=""
           F77GETARGDECL="external getarg"
	   F77GETARG="call getarg(i,s)"
	   F77IARGC="iargc()"
	   { echo "$as_me:$LINENO: Trying getarg and iargc" >&5
echo "$as_me: Trying getarg and iargc" >&6;}
	   ;;
	3) # Posix alternative
	   F90UNIX=""
	   F77GETARGDECL="external pxfgetarg"
	   F77GETARG="call pxfgetarg(i,s,l,ier)"
	   F77IARGC="ipxfiargc()"
	   { echo "$as_me:$LINENO: Trying pxfgetarg and ipxfiargc" >&5
echo "$as_me: Trying pxfgetarg and ipxfiargc" >&6;}
	   ;;
	4) # Nag f90_unix_env module
	   F90UNIX="        use f90_unix_env"
	   F77GETARGDECL=""
	   F77GETARG="call getarg(i,s)"
	   F77IARGC="iargc()"
	   { echo "$as_me:$LINENO: Trying f90_unix_env module" >&5
echo "$as_me: Trying f90_unix_env module" >&6;}
	   ;;
        5) # Nag f90_unix module
	   F90UNIX="        use f90_unix"
	   F77GETARGDECL=""
	   F77GETARG="call getarg(i,s)"
	   F77IARGC="iargc()"
	   { echo "$as_me:$LINENO: Trying f90_unix module" >&5
echo "$as_me: Trying f90_unix module" >&6;}
	   ;;
	6) # user spec in a file
	   if test -s f77argdef ; then
		. ./f77argdef
	       { echo "$as_me:$LINENO: Using definitions in the file f77argdef" >&5
echo "$as_me: Using definitions in the file f77argdef" >&6;}
	   else
		continue
	   fi
	   ;;
	esac
	# Create the program
        cat > conftest.f <<EOF
        program main
$F90UNIX
        integer i
        character*20 s

        $F77GETARGDECL
        $F77GETARG
        i=$F77IARGC
        end
EOF
	for fopt in $trialopts ; do
            if test "$msg_yes" = "yes" ; then break ; fi
	    FFLAGS_SAV="$FFLAGS"
	    case $fopt in
		0) ;;
		*) FFLAGS="$FFLAGS $fopt" ;;
	    esac
 	    # Try to compile
            echo "$F77 $FFLAGS -c -o conftest.o conftest.f" >> config.log
            if $F77 $FFLAGS -c -o conftest.o conftest.f >> config.log 2>&1 ; then
		:
	    else
	        # Could not compile sample program
	        echo "$as_me:$LINENO: result: could not compile test program" >&5
echo "${ECHO_T}could not compile test program" >&6
	        continue
	    fi
	    # We could *compile* the program.  Can we link it?
	    # Now for the possible libraries
            FLIBSAVE="$FLIBS"
	    for triallib in $triallibs ; do
  	        if test "$msg_yes" = "yes" ; then break ; fi
                rm -f conftest
                # msg_yes set to yes if the command line routine is found
                msg_yes=no
	        if test $triallib != "0" ; then
		    FLIBS="$triallib $FLIBS"
		    { echo "$as_me:$LINENO: linking with library $triallib" >&5
echo "$as_me: linking with library $triallib" >&6;}
	        fi

                echo "$FLINKER $FFLAGS -o conftest conftest.o $FLIBS" >> config.log
                if $FLINKER $FFLAGS -o conftest conftest.o $FLIBS >> config.log 2>&1 ; then
                    if test -x conftest ; then
	                echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6
                        msg_yes=yes
                    fi
  	        else
	            if test -s conftest.out ; then cat conftest.out >> config.log ; fi
                fi
	        rm -f conftest.out conftest
                # This format and library choice didn't work
                if test "$msg_yes" = "no" ; then
	             echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
	             FLIBS="$FLIBSAVE"
	         fi
             done
	     # This set of flags did not work with any library
	     if test "$msg_yes" = "no" ; then
	         # Just in case
                 FLIBS="$FLIBSAVE"
                 FFLAGS="$FFLAGS_SAVE"
             fi
         done
     # End of loop over tests
     done
     if test "$msg_yes" = "no" ; then
	F90UNIX=""
	F77GETARGDECL=""
	# We wanted to generate an error message, but we can't
	# put single quotes into a variable
	F77GETARG='print *, "'"'"'"ERROR - CANNOT FIND COMMAND LINE ARGS"'"'"'"'
	F77IARGC=1
	{ echo "$as_me:$LINENO: WARNING: Command line options will not work with Fortran. You will
		     not be able to run MPI programs if MPI_INIT is called from
		     Fortran." >&5
echo "$as_me: WARNING: Command line options will not work with Fortran. You will
		     not be able to run MPI programs if MPI_INIT is called from
		     Fortran." >&2;}
     fi
     rm -f conftest*


   # Check for strange behavior of Fortran.  For example, some FreeBSD
   # systems use f2c to implement f77, and the version of f2c that they
   # use generates TWO (!!!) trailing underscores
   # Currently, WDEF is not used but could be...
   #
   # Eventually, we want to be able to override the choices here and
   # force a particular form.  This is particularly useful in systems
   # where a Fortran compiler option is used to force a particular
   # external name format (rs6000 xlf, for example).
   cat > conftest.f <<EOF
